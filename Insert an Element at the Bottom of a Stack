class Solution{
public:
    stack<int> insertAtBottom(stack<int> st,int x)
    {
        stack<int> sat;
      while(!st.empty())
      {
          int top = st.top();
          st.pop();
          
          sat.push(top);
          if(st.empty())
          {
              sat.push(x);
          }
      }
      
      while(!sat.empty())
      {
          int top = sat.top();
          sat.pop();
          
          st.push(top);
      }
      
      return st;
        
    }
};
